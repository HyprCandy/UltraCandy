{ 
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "source": "$(find \"${XDG_CONFIG_HOME:-$HOME/.config}/fastfetch/pngs/\" -name \"*.png\" | shuf -n 1)",
    "height": 15,
    "width": 35,
      "padding": {
      "top": 3,
      "left": 0,
      "right": 1
      }
  },
  "display": {
        "separator": " "
  },
    "modules": [
        {
            "key": "╭───────────╮╭────────────────────────────────────────────────╮",
            "type": "custom"
        },
        {
            "key": "│ {#38} user    {#keys}││",
            "type": "title",
            "format": "User  {user-name}  Root  {host-name}"
        },
        {
            "key": "│ {#38}󰇄 device  {#keys}││",
            "type": "host",
            "format": "{5} {2}"
        },
        {
            "key": "│ {#33}{icon} distro  {#keys}││",
            "type": "os"
        },
        {
            "key": "│ {#34} kernel  {#keys}││",
            "type": "kernel"
        },
        {
            "key": "│ {#35}󱫐 wm      {#keys}││",
            "type": "wm"
        },
        {
            "key": "│ {#35} cursor  {#keys}││",
            "type": "cursor"
        },
        {
            "key": "│ {#36}󰀻 icons   {#keys}││",
            "type": "icons"
        },
        {
            "key": "│ {#32} term    {#keys}││",
            "type": "terminal"
        },
        {
            "key": "│ {#32} shell   {#keys}││",
            "type": "shell"
        },
        {
            "key": "│ {#36}󰏗 packages{#keys}││",
            "type": "packages"
        },
        {
            "key": "│ {#35}󰉉 disk    {#keys}││",
            "type": "disk"
        },
        {
            "key": "│ {#34}󰍛 memory  {#keys}││",
            "type": "memory"
        },
        {
            "key": "│ {#34}󰓡 swap    {#keys}││",
            "type": "swap"
        },
        {
            "key": "│ {#33}󰅐 os age  {#keys}││",
            "type": "disk",
            "folders": "/",
            "format": "{days} days"
        },
        {
            "key": "│ {#33}󰅐 uptime  {#keys}││",
            "type": "uptime"
        },
        {
            "key": "│ {#38}󰇺 cpu     {#keys}││",
            "type": "cpu",
            "showPeCoreCount": true
        },
        /*{
            "key": "│ {#32}󰇺 gpu     {#keys}││",
            "type": "gpu",
            "format": "{1} {2}"
        },*/
        {
            "key": "╰───────────╯╰────────────────────────────────────────────────╯",
            "type": "custom"
        },
        {
            "key": "╭───────────╮╭────────────────────────────────────────────────╮",
            "type": "custom"
        },
        {
            "key": "│ {#39} colors  {#keys}││                ",
            "type": "colors",
            "symbol": "circle"
        },
        {
            "key": "╰───────────╯╰────────────────────────────────────────────────╯",
            "type": "custom"
        },
    ]
}
